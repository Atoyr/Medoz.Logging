<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <VersionPrefix>0.1.0</VersionPrefix>

    <Title>Medoz.Logging</Title>
    <Authors>UCHIYAMA Ryota</Authors>
    <Owner>UCHIYAMA Ryota</Owner>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <RequireLicenseAcceptance>false</RequireLicenseAcceptance>
    <ProjectUrl>https://github.com/Atoyr/Medoz.Logging</ProjectUrl>
    <Description>.NET Logging tool.</Description>
    <ReleaseNotes>0.1.0 First Release</ReleaseNotes>
    <Copyright>© 2023 Ryota Uchiyama</Copyright>
    <Tags>.NET</Tags>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/Atoyr/Medoz.Logging</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Logging" Version="8.0.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\LICENSE" Pack="true" Visible="false" PackagePath="" />
    <None Include="..\.editorconfig" Link=".editorconfig" />
  </ItemGroup>
</Project>
